{
  "name": "duckai-openai-server",
  "version": "1.0.0",
  "description": "OpenAI-compatible HTTP server using Duck.ai backend",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "start": "tsx src/server.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:openai": "vitest run tests/openai-simple.test.ts",
    "test:openai-full": "vitest run tests/openai-library.test.ts",
    "test:tools": "vitest run tests/tool-service.test.ts tests/openai-tools.test.ts",
    "test:e2e": "vitest run tests/e2e-tools.test.ts",
    "test:all": "vitest run tests/server.test.ts tests/openai-simple.test.ts tests/tool-service.test.ts tests/openai-tools.test.ts"
  },
  "dependencies": {
    "jsdom": "^25.0.1",
    "openai": "^4.103.0",
    "user-agents": "^1.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/user-agents": "^1.0.4",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
